no = int(raw_input())
v = []
a=0;e=0;i=0;i=0;o=0;u=0;y=0
for i in range(int(no)):
    str = raw_input()
    a = str.count('a')
    e = str.count('e')
    i = str.count('i')
    o = str.count('o')
    u = str.count('u')
    y = str.count('y')
    v.append(a+e+i+o+u+y)
print(" ".join((str(e)) for e in v ))
#print(' '.join(map(str,v)))
